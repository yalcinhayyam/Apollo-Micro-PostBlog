{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "config/*": "./config",
          "middlewares/*": "./middlewares",
          "models/*": "./models",
          "public/*": "./public",
          "repositories/*": "./repositories",
          "services/*": "./services",
          "utils/*": "./utils",
          "database/*": "database/*"
        }
      }
    ]
  ]
}
